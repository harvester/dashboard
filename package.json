  "version": "0.1.0",
  "private": false,
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@rancher/components": "0.1.3",
    "@rancher/shell": "^0.3.19",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.184",
    "core-js": "3.21.1",
    "css-loader": "6.7.3",
    "@vue/vue2-jest": "27.0.0",
    "babel-jest": "27.5.1",
    "eslint-plugin-jest": "24.4.0",
    "jest": "27.5.1",
    "jest-serializer-vue": "2.0.2",
    "ts-jest": "27.1.4"
  },
  "devDependencies": {
    "yaml-lint": "1.7.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vue/cli-plugin-babel": "4.5.18",
    "@vue/cli-plugin-eslint": "4.5.18",
    "@vue/cli-plugin-typescript": "4.5.18",
    "@vue/cli-service": "4.5.18",
    "@vue/eslint-config-standard": "5.1.2",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "1.2.1",
    "babel-eslint": "10.1.0",
    "core-js": "3.25.3",
    "cron-validator": "1.2.0",
    "cronstrue": "1.95.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "9.10.0",
    "jsonpath-plus": "6.0.1",
    "vue-template-compiler": "2.7.14"
  },
  "resolutions": {
    "**/webpack": "4",
    "@types/node": "^16"
  },
  "scripts": {
    "dev": "NODE_ENV=dev ./node_modules/.bin/vue-cli-service serve",
    "build": "./node_modules/.bin/vue-cli-service build",
    "clean": "./node_modules/@rancher/shell/scripts/clean",
    "build-pkg": "./node_modules/@rancher/shell/scripts/build-pkg.sh",
    "serve-pkgs": "./node_modules/@rancher/shell/scripts/serve-pkgs",
    "publish-pkgs": "./node_modules/@rancher/shell/scripts/extension/publish",
    "lint": "./node_modules/.bin/eslint --max-warnings 0 --ext .js,.ts,.vue .",
    "test": "jest --watch",
    "test:ci": "jest --collectCoverage --silent",
    "install:ci": "yarn install --frozen-lockfile",
    "lint-l10n": "./node_modules/.bin/yamllint ./pkg/harvester/l10n"
  }
